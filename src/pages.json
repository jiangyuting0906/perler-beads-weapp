{
	"easycom": {
		"autoscan": true,
		"custom": {
			// uni-ui 规则如下配置
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home/index",
			"style": {
				"navigationBarTitleText": "拼豆管家"
			}
		},
		{
			"path": "pages/Inventory/index",
			"style": {
				"navigationBarTitleText": "库存管理"
			}
		},
		{
			"path": "pages/Supplement/index",
			"style": {
				"navigationBarTitleText": "库存补充"
			}
		},
		{
			"path": "pages/Consume/index",
			"style": {
				"navigationBarTitleText": "库存消耗"
			}
		},
		{
			"path": "pages/DataAnalysis/index",
			"style": {
				"navigationBarTitleText": "数据分析"
			}
		},
		{
			"path": "pages/History/index",
			"style": {
				"navigationBarTitleText": "历史记录"
			}
		},
		{
			"path": "pages/Remind/index",
			"style": {
				"navigationBarTitleText": "补豆提醒"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}